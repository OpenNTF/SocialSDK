<?xml version="1.0" encoding="UTF-8"?>
<faces-config>
  <!--
		Data acccess beans for the playground  
	 -->
  <managed-bean>
    <managed-bean-name>dataAccess</managed-bean-name>
    <managed-bean-class>playground.beans.DataAccessBean
		</managed-bean-class>
    <managed-bean-scope>application</managed-bean-scope>
  </managed-bean>
  <!--
		Beans for API Documentation capability  
	 -->
  <managed-bean>
    <managed-bean-name>docBean</managed-bean-name>
    <managed-bean-class>doc.DocAPI</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>docOptionsBean</managed-bean-name>
    <managed-bean-class>doc.OptionsBean
		</managed-bean-class>
    <managed-bean-scope>application</managed-bean-scope>
    <!--
			<managed-property> <property-name>playgroundUrl</property-name>
			<value>https://priand64.swg.usma.ibm.com/SBTPlayground.nsf</value>
			</managed-property>
		-->
  </managed-bean>
  <!--
		Bean holding the options for the whole playground. This is mostly for
		development and debugging purposes.
	-->
  <managed-bean>
    <managed-bean-name>optionsBean</managed-bean-name>
    <managed-bean-class>playground.beans.OptionsBean
		</managed-bean-class>
    <managed-bean-scope>application</managed-bean-scope>
  </managed-bean>
  <!--
		Bean used to dynamically compile and load dome XPages. This is mostly
		for development and debugging purposes.
	-->
  <managed-bean>
    <managed-bean-name>dynamicXPagesBean
		</managed-bean-name>
    <managed-bean-class>playground.beans.DynamicXPagesBean
		</managed-bean-class>
    <managed-bean-scope>application</managed-bean-scope>
  </managed-bean>
  <!--
		Managed beans containing the business logic when accessing an asset
		document.
	-->
  <managed-bean>
    <managed-bean-name>snippetBean</managed-bean-name>
    <managed-bean-class>playground.beans.JavaScriptSnippetBean
		</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>javaSnippetBean</managed-bean-name>
    <managed-bean-class>playground.beans.JavaSnippetBean
		</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>xpagesSnippetBean
		</managed-bean-name>
    <managed-bean-class>playground.beans.XPagesSnippetBean
		</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>gadgetSnippetBean
		</managed-bean-name>
    <managed-bean-class>playground.beans.GadgetSnippetBean
		</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>apiBean</managed-bean-name>
    <managed-bean-class>playground.beans.APIBean
		</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>importExport</managed-bean-name>
    <managed-bean-class>playground.beans.ImportExportBean
		</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  <!-- Credential store physical implementation -->
  <managed-bean>
    <managed-bean-name>CredStore</managed-bean-name>
    <managed-bean-class>com.ibm.sbt.security.credential.store.MemoryStore
		</managed-bean-class>
    <managed-bean-scope>application</managed-bean-scope>
  </managed-bean>
