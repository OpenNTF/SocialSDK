      <renderer-class>com.ibm.xsp.extlib.renderkit.html_extended.facebook.LoginRenderer</renderer-class>
    </renderer>   
   <renderer>
      <component-family>com.ibm.xsp.extlib.facebook.comments</component-family>
      <renderer-type>com.ibm.xsp.extlib.facebook.comments</renderer-type>
      <renderer-class>com.ibm.xsp.extlib.renderkit.html_extended.facebook.CommentsRenderer</renderer-class>
    </renderer>        
  </render-kit>
