<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   	<extension point="com.ibm.commons.Extension">
         <service type="com.ibm.xsp.Library" class="nsf.playground.library.PlaygroundLibrary" />
   	</extension>

    <extension point="com.ibm.commons.Extension">
         <service
         	type="com.ibm.sbt.playground.extension"
         	class="com.ibm.xsp.sbtsdk.playground.sbt.SbtPlaygroundExtensionFactory" />
    </extension>
    
    <extension point="com.ibm.pvc.webcontainer.application">
		<contextRoot>
			/osplayground
      	</contextRoot>
      	<contentLocation>
        	WebContent
      	</contentLocation>
   	</extension>

</plugin>
