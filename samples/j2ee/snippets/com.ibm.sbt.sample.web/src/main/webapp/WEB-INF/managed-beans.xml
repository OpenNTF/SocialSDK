		<managed-bean-scope>application</managed-bean-scope>
		<managed-property>
			<property-name>endpoints</property-name>
			<value>smartcloud:connections,smartcloud,smartcloudOA2,smartcloudBasic</value>
		</managed-property>
		<managed-property>
			<property-name>properties</property-name>
			<value>sample.email1,sample.email2</value>
		</managed-property>
		<managed-property>
            <property-name>runtimes</property-name>
            <value>smartcloud</value>
        </managed-property>
	</managed-bean>
	
